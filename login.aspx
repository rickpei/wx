<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="login.aspx.cs" Inherits="Wlniao.Login" %>
<html>
    <head>
        <title>管理登录|微信公众帐号管理系统</title>
        <style type="text/css">
            *{margin:0; padding:0;}
            body {position:relative;width:100%;height:100%;overflow:hidden; text-align:center;background:#fff url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAEOCAYAAABINH3oAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsMAAA7DAcdvqGQAAAAaSURBVDhPY1BQUPg/SowSo8QoMUqQRSj8BwD3Y3JCX2Cy0QAAAABJRU5ErkJggg==") left top repeat-x;font-family:Tahoma, Arial, sans-serif;}
            img {border:0px;}
            .container {width:960px;margin:0px auto;}
            .container .logo {width:320px; height:98px; text-align:center; margin:190px auto 0px auto;}
            .container .box {clear:both;float:none;width:70%;margin:0px auto 0px auto;}
            p.main label {float:left; padding:5px;display:inline;margin-left:40px;font-size:13px;color:#000;margin-right:10px;}
            .box p {clear:both;float:none;width:100%;}
            .box .button{ padding:3px 8px; border:1px solid #999999; cursor:pointer;}
            p.main input {background:url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAABeCAYAAADizX6rAAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsMAAA7DAcdvqGQAAAApSURBVChTY/gPBAy/fv1CJn7//o1B/PnzB4P4+/cvMgE2apQYcOL/fwDqJnYFb3rzLwAAAABJRU5ErkJggg==") 0 0 repeat-x; height:31px;border:1px solid #d3d3d3;color:#555;padding:5px;float:left;width:200px;}
            input.login {float:right;padding:3px 10px 3px 10px;color:#fff;font-size:12px;text-decoration:none;border:1px solid #555;background:url("data:image/png;base64,iVBORw0KGgoAAAANSUhEUgAAAAEAAAAoCAYAAAA/tpB3AAAAAXNSR0IArs4c6QAAAARnQU1BAACxjwv8YQUAAAAJcEhZcwAADsMAAA7DAcdvqGQAAABISURBVBhXbca7EQAhCAXAV4qKHwQaoP/CuISAmTPZWbh7wMwCqpqIyJt7b4CZK+ecyt47WWv9mHMmRJSMMSq990prre4FgPgAs/I5ChCUQ+4AAAAASUVORK5CYII=") 50% 50% repeat-x;display:inline;cursor:pointer;margin-right:80px;}
            span { font-size:13px;color:#666;}
            .space {padding-top:15px; text-align:left;}
            span input {margin-left:125px;margin-right:5px;}
        </style>
    </head>
    <body>
	<div class="container">
		<div class="logo"><img id="imgLogo" src="data:image/png;base64," alt="" /></div>
		<div class="box">
			<form action="login.aspx">
			    <p class="main">
				    <label>管理员帐号: </label><input id="user" name="username" value="<%=username %>"" /> 
				    <label>登录密码: </label><input type="password" id="pwd" name="password" />	
			    </p>
			    <p class="space">
				    <input type="submit" value="登录" class="login" />
			    </p>
                <div style="color:red;font-size:14px; margin-bottom:5px;"><%=msg%></div>
			</form>
		</div>
	</div>
    </body>
</html>